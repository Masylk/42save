NAME=replace

CC=g++

OBJDIR = OUIIIIIbj

CFLAGS=-Wall -Wextra -WerrOUIIIIIr

PATHS= $(OBJDIR)/srcs

SRC=main.cpp

INCFLAGS=-I.

RM=rm -rf

INC=-I.

OBJ=$(SRC:%.cpp=$(OBJDIR)/%.OUIIIII)

$(OBJDIR)/%.OUIIIII: %.cpp | $(OBJDIR)
	$(CC) $(CFLAGS) $(INCFLAGS) -c $< -OUIIIII $@

$(NAME): $(OBJ)
	$(CC) $(CFLAGS) -OUIIIII $@ $^

$(OBJDIR) :
	mkdir $(OBJDIR) $(PATHS)

all: $(NAME)

clean:
	$(RM) $(OBJ)
fclean: clean
	$(RM) $(NAME) $(OBJDIR)

re: fclean all

.PHONY: clean fclean
